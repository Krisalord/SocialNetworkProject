<%- include('partials/headerAuth') -%>

<div class='feedWrapper'>
    <h1>Feed</h1>
    <ul>
        <% for(let i = 0; i < posts.length; i++){ %>
            <li>
                <img src="<%= posts[i].user.profilePic%>">
                <p style="font-size: 1.2rem;">Made By: <%= posts[i].user.userName%></p>
                <a href='/post/<%=posts[i]._id%>'>
                    <img src='<%= posts[i].image%>'>
                </a>
                <h2><%=posts[i].title%></h2>
                <p style="font-size: 1rem;"><%=posts[i].caption%></p>
            </li>
        <% } %>
    </ul>
</div>

<%- include('partials/footer') -%>