<%- include('partials/headerAuth') -%>

<div class='feedWrapper'>
    <h1>Feed</h1>
    <ul>
        <% for(let i = 0; i < posts.length; i++){ %>
            <li>
                <div class="postHeader">
                    <div class="postMadeBy">
                        <img src="<%= posts[i].user.profilePic%>" class="smallPfp">
                        <p><%= posts[i].user.userName%></p>
                    </div>
                    <div class="madeDate"><p>11.21.1221</p></div>
                </div>
                
                <a href='/post/<%=posts[i]._id%>'>
                    <img src='<%= posts[i].image%>' class="postImg">
                </a>
                <div class="title_caption">
                    <h2><%=posts[i].title%></h2><br>
                    <p><%=posts[i].caption%></p>
                </div>
                <div class="likes_post">
                    
                </div>
                <div class="commentsDivider"></div>
            </li>
        <% } %>
    </ul>
</div>

<%- include('partials/footer') -%>